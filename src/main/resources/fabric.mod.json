{
  "schemaVersion": 1,
  "id": "spacemod",
  "version": "${version}",
  "name": "spacemod",
  "description": "the spacemod",
  "authors": [],
  "contact": {
    "repo": "https://github.com/Aedhan Scott/fabricspacemodabricspacemod"
  },
  "license": "MIT",
  "icon": "assets/spacemod/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "org.example.stardust.spacemod.client.SpaceModClient"
    ],
    "main": [
      "org.example.stardust.spacemod.SpaceMod"
    ],
    "rei_client": [
      "org.example.stardust.spacemod.compat.SpaceModREIClientPlugin"
    ]
  },

  "mixins": [
    "spacemod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
